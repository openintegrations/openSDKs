{
  "name": "@opensdks/sdk-openai",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "./index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "**/*.ts",
    "!**/*.spec.ts",
    "!**/*.json"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:dts": "mkdir -p dist && cp *.d.ts ./dist",
    "build:json": "mkdir -p dist && npx tsx ../../bin/oasJsonToJs.ts ./ ./dist",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "download": "curl --fail https://raw.githubusercontent.com/openai/openai-openapi/master/openapi.yaml -o ./openai.oas.yaml",
    "generate": "run-s generate:*",
    "generate:json": "cat openai.oas.yaml | node ../../bin/yaml2json.js -o openai.oas",
    "generate:types": "openapi-typescript ./openai.oas.json --output ./openai.oas.d.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module"
}
