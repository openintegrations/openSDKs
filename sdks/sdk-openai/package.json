{
  "name": "@opensdks/sdk-openai",
  "version": "0.0.3",
  "type": "module",
  "imports": {
    "#module/*": "./*"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*.oas.js": "./*.oas.js",
    "./*.oas.json": "./*.oas.json",
    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.json",
    "!**/*.spec.ts",
    "!dist/**/*.json"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:cjs": "tsc -p ./tsconfig.json --declaration false --declarationMap false --module CommonJS --moduleResolution Node10 --outDir ./dist/cjs",
    "build:cjs-pkgjson": "mkdir -p ./dist/cjs && echo '{\"type\": \"commonjs\"}' > ./dist/cjs/package.json",
    "build:esm": "tsc -p ./tsconfig.json --declaration false --declarationMap false --outDir ./dist/esm",
    "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly --outDir ./dist/types",
    "clean": "rm -rf ./dist",
    "download": "curl --fail https://raw.githubusercontent.com/openai/openai-openapi/master/openapi.yaml -o ./openai.oas.yaml",
    "generate": "run-s generate:*",
    "generate:json": "cat openai.oas.yaml | node ../../bin/yaml2json.js -o openai.oas",
    "generate:types": "npx tsx ../../packages/cli/generateFromOas.ts ./openai.oas.json",
    "version": "pnpm version"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
